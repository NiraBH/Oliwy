<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="oliwy.com.oliwy.ContactosActivity">
<ScrollView
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:orientation="vertical"
        android:weightSum="1">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="5dp"
            android:gravity="center"
            android:text="CONTACTOS"
            android:textColor="@android:color/black"
            android:textSize="22sp"
            android:textStyle="bold"
            android:layout_marginTop="30dp"
            />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_weight="0.12"
            android:gravity="center"
            android:orientation="horizontal"
            android:layout_marginTop="10dp">

            <Button
                android:id="@+id/boton1"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Iulian"
                />

            <Button
                android:id="@+id/boton2"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Nira"

                />

            <Button
                android:id="@+id/boton3"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Cristina"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="15dp"
            android:layout_weight="0.12"
            android:gravity="center"
            android:orientation="horizontal">

            <Button
                android:id="@+id/boton4"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Diego"/>

            <Button
                android:id="@+id/boton5"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Alvaro E." />

            <Button
                android:id="@+id/boton6"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Alvaro G."/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="15dp"
            android:layout_weight="0.12"
            android:gravity="center_horizontal">

            <Button
                android:id="@+id/boton7"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Aitor"/>

            <Button
                android:id="@+id/boton8"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Daniel"/>

            <Button
                android:id="@+id/boton9"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Cristian"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="15dp"
            android:layout_weight="0.12"
            android:gravity="center_horizontal">

            <Button
                android:id="@+id/boton10"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Antonio"/>

            <Button
                android:id="@+id/boton11"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Isabel"

                />

            <Button
                android:id="@+id/boton12"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Zaineb"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="15dp"
            android:layout_weight="0.12"
            android:gravity="center_horizontal">

            <Button
                android:id="@+id/boton13"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Javier"/>

            <Button
                android:id="@+id/boton14"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Vale" />

            <Button
                android:id="@+id/boton15"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Rodrigo"/>

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="15dp"
            android:layout_marginTop="15dp"
            android:layout_weight="0.12"
            android:gravity="center_horizontal">

            <Button
                android:id="@+id/boton16"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Dante"/>

            <Button
                android:id="@+id/boton17"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Susana"
                />

            <Button
                android:id="@+id/boton18"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="Carlota"/>

        </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="15dp"
        android:layout_marginTop="15dp"
        android:layout_weight="0.12"
        android:gravity="center_horizontal">

        <Button
            android:id="@+id/boton19"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:text="Manuel"/>

        </LinearLayout>
    </LinearLayout>

</ScrollView>
</LinearLayout>
